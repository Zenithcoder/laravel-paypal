{
    "name": "zenithcoder/laravel-paypal",
    "description": "laravel paypal integration for payment processing",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Awonusi Olajide",
            "email": "awonusiolajide@yahoo.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.3"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Zenithcoder\\Paypal\\": "src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "autoload-dev": {
        "psr-4": {
            "Zenithcoder\\Paypal\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zenithcoder\\Paypal\\PaypalServiceProvider"
            ],
            "aliases": {
                "Paypal": "Zenithcoder\\Paypal\\Facades\\Paypal"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1",
        "satooshi/php-coveralls": "^0.7.0",
        "mockery/mockery": "^1.0"
    }
}